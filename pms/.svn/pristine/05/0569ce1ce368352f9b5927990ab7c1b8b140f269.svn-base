package cn.teacheredu.entity;

import java.math.BigDecimal;

public class ProjectYearSummaryEntity extends BaseEntity {
    /**
	 * 
	 */
	private static final long serialVersionUID = -2577908488138009564L;

	private Integer id;

    private Integer projectId;

    private Integer year;

    private BigDecimal incomeamount;

    private BigDecimal overamount;

    private BigDecimal paymentamount;

    private BigDecimal fundsamount;

    private BigDecimal invoiceamount;

    private BigDecimal budgetamount;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getProjectId() {
        return projectId;
    }

    public void setProjectId(Integer projectId) {
        this.projectId = projectId;
    }

    public Integer getYear() {
        return year;
    }

    public void setYear(Integer year) {
        this.year = year;
    }

    public BigDecimal getIncomeamount() {
        return incomeamount;
    }

    public void setIncomeamount(BigDecimal incomeamount) {
        this.incomeamount = incomeamount;
    }

    public BigDecimal getOveramount() {
        return overamount;
    }

    public void setOveramount(BigDecimal overamount) {
        this.overamount = overamount;
    }

    public BigDecimal getPaymentamount() {
        return paymentamount;
    }

    public void setPaymentamount(BigDecimal paymentamount) {
        this.paymentamount = paymentamount;
    }

    public BigDecimal getFundsamount() {
        return fundsamount;
    }

    public void setFundsamount(BigDecimal fundsamount) {
        this.fundsamount = fundsamount;
    }

    public BigDecimal getInvoiceamount() {
        return invoiceamount;
    }

    public void setInvoiceamount(BigDecimal invoiceamount) {
        this.invoiceamount = invoiceamount;
    }

    public BigDecimal getBudgetamount() {
        return budgetamount;
    }

    public void setBudgetamount(BigDecimal budgetamount) {
        this.budgetamount = budgetamount;
    }
}