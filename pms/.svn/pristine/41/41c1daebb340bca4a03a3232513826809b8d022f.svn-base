@charset "utf-8";
/*reset*/
body,
h1,
h2,
h3,
h4,
h5,
p,
dl,
dd,
ul,
ol,
form,
input,
textarea,
th,
td,
select {
  margin: 0;
  padding: 0;
}
h1,
h2,
h3,
h4,
h5 {
  font-size: 100%;
  font-weight: normal;
}
em {
  font-style: normal;
}
strong {
  font-style: normal;
}
i {
  font-style: normal;
}
li {
  list-style: none;
}
img {
  border: none;
  vertical-align: top;
}
table {
  border-collapse: collapse;
}
textarea {
  resize: none;
  overflow: auto;
}
span {
  word-wrap: break-word;
}
input,
textarea {
  outline: none;
}
a {
  text-decoration: none;
}
a:active {
  outline: 0;
}
body {
  font-size: 12px;
  line-height: 22px;
  color: #545454;
  background: #efefef;
}
input[type="text"],
input[type="password"] {
  font-size: 14px;
  border: 1px solid #abadb3;
  padding: 5px;
  height: 22px;
  line-height: 32px;
}
select {
  border: 1px solid #abadb3;
}
